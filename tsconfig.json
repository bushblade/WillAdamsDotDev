{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": ["@astrojs/image/client"],
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@scripts/*": ["src/scripts/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "jsxImportSource": "solid-js"
  }
}
